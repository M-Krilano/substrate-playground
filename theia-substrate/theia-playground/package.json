        "@theia/editor-preview": "0.16.1",
        "@theia/file-search": "0.16.1",
        "@theia/git": "0.16.1",
        "@theia/json": "0.16.1",
        "@theia/keymaps": "0.16.1",
        "@theia/markers": "0.16.1",
        "@theia/messages": "0.16.1",
        "@theia/metrics": "0.16.1",
        "@theia/mini-browser": "0.16.1",
        "@theia/monaco": "0.16.1",
        "@theia/navigator": "0.16.1",
        "@theia/outline-view": "0.16.1",
        "@theia/output": "0.16.1",
        "@theia/plugin": "0.16.1",
        "@theia/plugin-dev": "0.16.1",
        "@theia/plugin-ext": "0.16.1",
        "@theia/plugin-ext-vscode": "0.16.1",
        "@theia/preview": "0.16.1",
        "@theia/preferences": "0.16.1",
        "@theia/process": "0.16.1",
        "@theia/search-in-workspace": "0.16.1",
        "@theia/task": "0.16.1",
        "@theia/terminal": "0.16.1",
        "@theia/typehierarchy": "0.16.1",
        "@theia/userstorage": "0.16.1",
        "@theia/variable-resolver": "0.16.1",
        "@theia/workspace": "0.16.1",
        "@parity/theia-playground-extension": "0.5.0"
    },
    "devDependencies": {
        "@theia/cli": "0.16.1",
        "@theia/debug": "0.16.1"
    },
    "resolutions": {
        "vscode-languageserver-protocol": "3.15.0-next.9",
        "vscode-languageserver-types": "3.15.0-next.5",
