[
    {
      "op": "add",
      "path": "/spec/rules/-",
      "value": {
        "host": "%HOST%",
        "http": {
          "paths": [
            {
              "path": "/",
              "backend": {
                "serviceName": "%SERVICE_NAME%",
                "servicePort": 3000
              }
            },
            {
              "path": "/front-end",
              "backend": {
                "serviceName": "%SERVICE_NAME%",
                "servicePort": 8000
              }
            },
            {
              "path": "/wss",
              "backend": {
                "serviceName": "%SERVICE_NAME%",
                "servicePort": 9944
              }
            }
          ]
        }
      }
    }
  ]