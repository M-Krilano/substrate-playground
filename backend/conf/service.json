{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "generateName": "theia-substrate-http-",
        "labels": {
            "app": "theia-substrate",
            "app-uuid": "%UUID%"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "web",
                "protocol": "TCP",
                "port": 80
            },
            {
                "name": "front-end",
                "protocol": "TCP",
                "port": 8000
            },
            {
                "name": "http",
                "protocol": "TCP",
                "port": 9933
            },
            {
                "name": "ws",
                "protocol": "TCP",
                "port": 9944
            }
        ],
        "selector": {
            "app-uuid": "%UUID%"
        },
        "type": "NodePort"
    }
}